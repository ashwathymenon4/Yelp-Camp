<% layout('layouts/boilerplate') %>
    <div class="container">
        <div class="row">
            <div class="col-6 offset-2 d-flex">
                <div class="alert alert-danger" role="alert">
                    <h4 class="alert-heading">
                        <%= err.message %>
                    </h4>
                    <% if (process.env.NODE_ENV !=="production" ) {%>
                        <p class="mb-0">
                            <%= err.stack %>
                        </p>
                        <% } %>
                </div>
            </div>
        </div>
    </div>